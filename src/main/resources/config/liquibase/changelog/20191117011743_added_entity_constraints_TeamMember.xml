<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity TeamMember.
    -->
    <changeSet id="20191117011743-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="team_member"
                                 constraintName="fk_team_member_tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"/>

        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="team_member"
                                 constraintName="fk_team_member_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="activity_id"
                                 baseTableName="team_member"
                                 constraintName="fk_team_member_activity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activity"/>

    </changeSet>
</databaseChangeLog>
