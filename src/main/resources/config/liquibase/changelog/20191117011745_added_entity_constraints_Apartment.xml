<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Apartment.
    -->
    <changeSet id="20191117011745-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="internet_access_id"
            baseTableName="apartment"
            constraintName="fk_apartment_internet_access_id"
            referencedColumnNames="id"
            referencedTableName="internet_access"/>

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="apartment"
                                 constraintName="fk_apartment_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

    </changeSet>
</databaseChangeLog>
