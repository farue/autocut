<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity WashHistory.
    -->
    <changeSet id="20200326184247-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="coin_id"
                                 baseTableName="wash_history"
                                 constraintName="fk_wash_history_coin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="coin"/>

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="wash_history"
                                 constraintName="fk_wash_history_tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"/>

        <addForeignKeyConstraint baseColumnNames="machine_id"
                                 baseTableName="wash_history"
                                 constraintName="fk_wash_history_machine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="laundry_machine"/>

        <addForeignKeyConstraint baseColumnNames="program_id"
                                 baseTableName="wash_history"
                                 constraintName="fk_wash_history_program_id"
                                 referencedColumnNames="id"
                                 referencedTableName="laundry_machine_program"/>

    </changeSet>
</databaseChangeLog>
