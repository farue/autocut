<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity SecurityPolicy.
    -->
    <changeSet id="20191117011747-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="team_member_id"
            baseTableName="security_policy"
            constraintName="fk_security_policy_team_member_id"
            referencedColumnNames="id"
            referencedTableName="team_member"/>

        <addForeignKeyConstraint baseColumnNames="tenant_id"
                                 baseTableName="security_policy"
                                 constraintName="fk_security_policy_tenant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tenant"/>

    </changeSet>
</databaseChangeLog>
